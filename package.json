{
  "name": "yellfy-loader",
  "version": "2.0.2",
  "description": "User-friendly Gulp task manager for Yellfy project.",
  "license": "MIT",
  "repository": "mrmlnc/yellfy-loader",
  "author": {
    "name": "Denis Malinochkin",
    "url": "canonium.com"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "out/loader.js",
  "typings": "out/loader.d.ts",
  "files": [
    "index.js",
    "out/loader.js",
    "out/loader.d.ts"
  ],
  "keywords": [
    "yellfy",
    "loader",
    "gulp",
    "tasks",
    "manager"
  ],
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "@types/node": "0.0.2",
    "gulp": "gulpjs/gulp#4.0",
    "mocha": "^3.0.1",
    "rimraf": "^2.5.0",
    "tslint": "^5.1.0",
    "typescript": "^2.0.3"
  },
  "scripts": {
    "clean": "rimraf out",
    "lint": "tslint src/**/*.ts",
    "compile": "tsc",
    "test": "rimraf .tmp && mocha out/**/*.spec.js",
    "build": "npm run clean && npm run lint && npm run compile && npm test ",
    "watch": "npm run clean && npm run lint && npm run compile -- --sourceMap --watch"
  }
